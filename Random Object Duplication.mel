global proc string BlockD(){

string $Poly[] = `ls -selection -flatten`;
string $Blocks[];
int $randAmount = rand (1, 50);
float $x = rand(1,10);
float $y = rand(1,10);
float $z = rand(1,10);

if (size($Poly) <= 0)
    warning "No object selected.";
else 
{
    for ($i=0;$i < $randAmount; $i++)
    {
        duplicate -rr; 
        move -ws -wd -x = `$x` -y = `$y` -z = `$z` $Blocks;
     }
}
return $Blocks;
}


