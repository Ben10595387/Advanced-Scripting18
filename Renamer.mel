$window = `window -title "Rename Selected Objects" //look into dockControl 
       -widthHeight 300 200`;
    columnLayout-adjustableColumn true;
    text -label "Input Name";
    textField -edit -enterCommand "RenameSelectedObject(string $input)";   
    button -label "Go" -command RenameSelectedObject;//Calls a procedure on click. 
        
showWindow $window;